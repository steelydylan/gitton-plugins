{
  "name": "@gitton-dev/plugin-zenn-markdown",
  "version": "0.0.16",
  "description": "Zenn-style markdown rendering support for Gitton",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf ui node_modules/.vite"
  },
  "gitton": {
    "displayName": "Zenn Markdown",
    "version": "0.0.1",
    "description": "Render markdown with Zenn-style syntax (:::message, :::details, @[youtube], etc.)",
    "author": "Gitton Team",
    "permissions": [
      "ui:settings",
      "ui:markdown"
    ],
    "extensionPoints": {
      "markdown": {
        "name": "Zenn Markdown",
        "filePatterns": [
          "*.md",
          "*.mdx"
        ],
        "priority": 10,
        "entry": "ui/src/markdown/index.html"
      },
      "settingsTab": {
        "entry": "ui/src/settings/index.html",
        "label": "Zenn Markdown"
      }
    },
    "security": {
      "allowedDomains": [
        {
          "domain": "https://unpkg.com",
          "types": [
            "script",
            "style",
            "font"
          ],
          "description": "KaTeX math rendering resources"
        },
        {
          "domain": "https://embed.zenn.studio",
          "types": [
            "frame",
            "script"
          ],
          "description": "Zenn embed server (card, tweet, gist, mermaid)"
        },
        {
          "domain": "https://www.youtube-nocookie.com",
          "types": [
            "frame"
          ],
          "description": "YouTube video embeds (privacy-enhanced mode)"
        },
        {
          "domain": "https://speakerdeck.com",
          "types": [
            "frame"
          ],
          "description": "Speaker Deck slide embeds"
        },
        {
          "domain": "https://www.slideshare.net",
          "types": [
            "frame"
          ],
          "description": "SlideShare slide embeds"
        },
        {
          "domain": "https://codepen.io",
          "types": [
            "frame"
          ],
          "description": "CodePen embeds"
        },
        {
          "domain": "https://codesandbox.io",
          "types": [
            "frame"
          ],
          "description": "CodeSandbox embeds"
        },
        {
          "domain": "https://stackblitz.com",
          "types": [
            "frame"
          ],
          "description": "StackBlitz embeds"
        },
        {
          "domain": "https://www.figma.com",
          "types": [
            "frame"
          ],
          "description": "Figma design embeds"
        },
        {
          "domain": "https://jsfiddle.net",
          "types": [
            "frame"
          ],
          "description": "JSFiddle embeds"
        },
        {
          "domain": "https://docswell.com",
          "types": [
            "frame"
          ],
          "description": "Docswell slide embeds"
        },
        {
          "domain": "https://blueprintue.com",
          "types": [
            "frame"
          ],
          "description": "BlueprintUE embeds"
        }
      ]
    }
  },
  "files": [
    "ui",
    "package.json"
  ],
  "keywords": [
    "gitton",
    "gitton-plugin",
    "zenn",
    "markdown"
  ],
  "author": "godai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:gitton-dev/gitton-plugins.git",
    "directory": "packages/plugin-zenn-markdown"
  },
  "bugs": {
    "url": "https://github.com/gitton-dev/gitton-plugins/issues"
  },
  "homepage": "https://jsers.dev/service/gitton",
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "postcss": "^8.5.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-node-polyfills": "^0.25.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "zenn-markdown-html": "^0.1.160"
  }
}
